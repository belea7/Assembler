; file ps.as

.entry LOOP
.entry LENGTH
.extern L3
.extern L3
.extern W
MAIN:       mov     S1.1
            add     r2 STR
            jmp
            prn     #-5
            sub     r1,r4
            inc     K

            mov     S1.2,r3
            bne     L3
3END:        stop
STR:        .string "abcdef"
LENGTH:     .data   6,-9,,15
K:          .data
S1:         .struct 8a, "ab"